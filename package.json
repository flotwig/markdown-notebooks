{
  "name": "markdown-notebooks",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@svgr/webpack": "4.3.3",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.6",
    "babel-preset-react-app": "7.0.2",
    "bfj": "6.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "2.4.2",
    "css-loader": "2.1.1",
    "cypress": "3.8.3",
    "cypress-get-react-component": "1.0.0-dev4",
    "cypress-react-unit-test": "2.3.2",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react-dev-utils": "7.0.3",
    "resolve": "1.12.2",
    "sass-loader": "7.3.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.4.1",
    "url-loader": "1.1.2",
    "wait-on": "3.3.0",
    "webpack": "4.41.2",
    "webpack-dev-server": "3.9.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "3.6.3"
  },
  "dependencies": {
    "@blueprintjs/core": "3.24.0",
    "@blueprintjs/icons": "3.14.0",
    "history": "4.10.1",
    "left-pad": "1.3.0",
    "marked": "0.6.1",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-app-polyfill": "0.2.2",
    "react-dom": "16.12.0",
    "react-redux": "6.0.1",
    "react-split-pane": "2.0.2",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-starter-kit": "0.9.1",
    "redux-thunk": "2.3.0",
    "sanitize-html": "1.22.1",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "cypress:start": "GITHUB_BASEURL=/__github/ yarn start",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:ci": "yarn cypress:start & wait-on http://localhost:3000 && yarn cypress:run -- --record",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "stop-only": "stop-only --folder cypress/integration --folder test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
